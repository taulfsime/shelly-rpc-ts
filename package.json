{
  "name": "@taulfsime/shelly-rpc-ts",
  "version": "0.2.0",
  "description": "Type definitions for the Shelly RPC interface and helper functions and classes",
  "keywords": [
    "shelly",
    "typescript"
  ],
  "homepage": "https://github.com/taulfsime/shelly-rpc-types#readme",
  "bugs": {
    "url": "https://github.com/taulfsime/shelly-rpc-types/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taulfsime/shelly-rpc-types.git"
  },
  "license": "MIT",
  "author": "taulfsime",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "format:check": "prettier . --check",
    "format": "prettier . --write --list-different",
    "types:check": "tsc --noEmit",
    "build": "rm -rf dist && tsc",
    "tests": "vitest run"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "vitest": "^3.1.2"
  }
}
